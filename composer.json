{
    "name": "dorcas/modules-people",
    "description": "Dorcas Hub People Module",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Ifeoluwa Olawoye",
            "email": "bolaji.olawoye@hostvillenigeria.org"
        },
        {
            "name": "Ayansola Olawuyi",
            "email": "zaccheausolawuyi@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|8.1.7",
        "aws/aws-sdk-php": "^3.74",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Dorcas\\ModulesPeople\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dorcas\\ModulesPeople\\ModulesPeopleServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
